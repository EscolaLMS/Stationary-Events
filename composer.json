{
  "name": "escolalms/stationary-events",
  "description": "Escola Headless LMS Stationary Events",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "escolalms/auth": "^0",
    "escolalms/core": "^1",
    "escolalms/categories": "^0",
    "escolalms/files": "^0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=6",
    "nunomaduro/larastan": "^2.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Maciej Rymarz",
      "email": "maciej.rymarz@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\StationaryEvents\\": "src",
      "EscolaLms\\StationaryEvents\\Tests\\": "tests",
      "EscolaLms\\StationaryEvents\\Database\\Factories\\": "database/factories",
      "EscolaLms\\StationaryEvents\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\StationaryEvents\\EscolaLmsStationaryEventsServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
